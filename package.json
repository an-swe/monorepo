{
  "name": "anprogrammer-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:link": "npm run dev --workspace=apps/link",
    "deploy:link": "npm run deploy --workspace=apps/link",
    "dev:index": "npm run dev --workspace=apps/index",
    "deploy:index": "npm run deploy --workspace=apps/index"
  }
}
